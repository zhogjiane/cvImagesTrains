   "source": [
    "import cv2\n",
    "\n",
    "cap = cv2.VideoCapture(0)\n",
    "\n",
    "while True:\n",
    "    ret, frame = cap.read() \n",
    "\n",
    "    if ret:\n",
    "        frame = cv2.resize(frame, (0, 0), fx=1.2, fy=1.2)\n",
    "        cv2.imshow('video', frame)\n",
    "    else:\n",
    "        break\n",
    "    if cv2.waitKey(10) == ord('q'):\n",
    "        break"
   ]
